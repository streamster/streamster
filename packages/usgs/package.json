  },
  "devDependencies": {
    "@types/papaparse": "^5.0.3",
    "babel-jest": "^26.6.3",
    "husky": "^4.2.5",
    "lerna": "^3.22.1",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "ajv": "^6.12.6",
    "axios": "^0.21.1"
